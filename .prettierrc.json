{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "trailingComma": "all",
  "bracketSameLine": false,
  "printWidth": 80,
  "plugins": [
    "prettier-plugin-multiline-arrays"
  ],
  "multilineArraysWrapThreshold": 1
}
